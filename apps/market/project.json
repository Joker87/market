{
  "name": "market",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "app",
  "sourceRoot": "apps/market/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:application",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/apps/market",
        "index": "apps/market/src/index.html",
        "browser": "apps/market/src/main.ts",
        "polyfills": [
          "zone.js"
        ],
        "tsConfig": "apps/market/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/market/src/favicon.ico",
          "apps/market/src/assets",
          "apps/market/src/manifest.webmanifest"
        ],
        "styles": [
          "@angular/material/prebuilt-themes/pink-bluegrey.css",
          "apps/market/src/styles.scss"
        ],
        "scripts": [],
        "server": "apps/market/src/main.server.ts",
        "prerender": true,
        "ssr": {
          "entry": "apps/market/server.ts"
        }
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all",
          "serviceWorker": "apps/market/ngsw-config.json"
        },
        "development": {
          "optimization": false,
          "extractLicenses": false,
          "sourceMap": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "market:build:production"
        },
        "development": {
          "buildTarget": "market:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "market:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "apps/market/jest.config.ts"
      }
    }
  }
}
